@model List<Mvc_basic_1.Models.Services.PeopleService>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

@{
    ViewData["Title"] = "PeopleList";
}
<h1>PeopleList</h1>

<div class="col-12">
    <div class="row">
        <div class="col">
            <h5>Name</h5>
        </div>
        <div class="col">
            <h5>Country</h5>
        </div>
    </div>

    @foreach (PeopleService item in Model)
    {
        <div class="row">
            <div class="col">
                <p>@item</p>
            </div>
            <div class="col">
                <p>@item</p>
            </div>
        </div>
    }
</div>