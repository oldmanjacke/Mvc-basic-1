
@{
    ViewData["Title"] = "PersonList";
}

<h1>PersonList</h1>

<div class="col-12">
    <div class="row">
        <div class="col">
            <h5>Name</h5>
        </div>
        <div class="col">
            <h5>Country</h5>
        </div>
    </div>

    @foreach (var item in Model)
    {
        <div class="row">
            <div class="col">
                <p>@item.Name</p>
            </div>
            <div class="col">
                <p>@item.Country</p>
            </div>
        </div>
    }
</div> 